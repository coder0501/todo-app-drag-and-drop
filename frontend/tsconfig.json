{
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "strict": true,
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "skipLibCheck": true

  },
  "include": ["src", "src/types"]
}


